{
  "firstPage": "StepsPage",
  "pages": {
    "StepsPage": {
      "wipeExistingData": true,
      "type": "flat",
      "title": "CreateContact",
      "mode": "steps",
      "upsert": {
        "validator": []
      },
      "pageDataExpression": "formData.Data",
      "defaultPageData": {
        "data": {
          "PreferMailing": false
        },
        "objectName": "ShowcaseCreateContactObject"
      },
      "stepDef": {
        "stepValue": "pageData.__step",
        "steps": [{
          "key": "first",
          "ui": {
            "items": [
              {
                "type": "textEditor",
                "valueExpression": "pageData.FirstName",
                "title": "FirstName",
                "placeholder": "FirstNamePlaceholder",
                "validator": [{
                  "type": "expression",
                  "errorMessage": "FieldIsRequired",
                  "expression": "pageData.FirstName"
                }]
              },
              {
                "type": "textEditor",
                "valueExpression": "pageData.LastName",
                "title": "LastName",
                "placeholder": "LastNamePlaceholder",
                "validator": [{
                  "type": "expression",
                  "errorMessage": "FieldIsRequired",
                  "expression": "pageData.LastName"
                }]
              }
            ]
          },
          "nextStepRoutingLogic": [{
            "key": "second"
          }]
        }, {
          "key": "second",
          "ui": {
            "items": [
              {
                "type": "textEditor",
                "valueExpression": "pageData.Phone",
                "title": "Phone",
                "placeholder": "PhonePlaceholder",
                "keyboardType": "phone-pad",
                "validator": [{
                  "type": "expression",
                  "errorMessage": "FieldIsRequired",
                  "expression": "pageData.Phone"
                }]
              },
              {
                "type": "textEditor",
                "valueExpression": "pageData.Email",
                "title": "Email",
                "placeholder": "EmailPlaceholder",
                "keyboardType": "email-address",
                "validator": [{
                  "type": "expression",
                  "errorMessage": "FieldIsRequired",
                  "expression": "pageData.Email"
                }]
              },
              {
                "type": "toggleEditor",
                "mode": "switch",
                "items": [
                  {
                    "valueExpression": "pageData.PreferMailing",
                    "text": "PreferMailing"
                  }
                ],
                "caption": "PreferMailingPlaceholder"
              }
            ]
          },
          "nextStepRoutingLogic": [{
            "condition": "pageData.PreferMailing",
            "key": "third"
          }]
        },
          {
            "key": "third",
            "ui": {
              "items": [
                {
                  "type": "textEditor",
                  "valueExpression": "pageData.MailingStreet",
                  "title": "Street",
                  "placeholder": "StreetPlaceholder"
                },
                {
                  "type": "textEditor",
                  "valueExpression": "pageData.MailingCity",
                  "title": "City",
                  "placeholder": "CityPlaceholder"
                },
                {
                  "type": "textEditor",
                  "valueExpression": "pageData.MailingState",
                  "title": "State",
                  "placeholder": "StatePlaceholder"
                },
                {
                  "type": "textEditor",
                  "valueExpression": "pageData.MailingZipCode",
                  "title": "ZipCode",
                  "placeholder": "ZipCodePlaceholder"
                }
              ]
            }
          }]
      }
    }
  }
}
