{
  "type": "GraphQl",
  "JobProduct": {
    "limit": 1,
    "fields": [
      "UID",
      "ProductId",
      "Name",
      "Qty",
      "CreatedDate",
      "JobId",
      "CreatedById"
    ],
    "filter": "CreatedById == '${resourceId}' AND JobId == '${jobId}'",
    "object": "JobProducts",
    "orderBy": "CreatedDate DESC",
    "variables": {
      "jobId": "$it.UID",
      "resourceId": "$res.UID"
    }
  }
}